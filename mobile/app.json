{
  "expo": {
    "name": "dsocial",
    "slug": "dsocial",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tech.berty.dsocial",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "tech.berty.dsocial.ios",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "exp.direct": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      },
      "buildNumber": "9"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "tech.berty.dsocial.android",
      "versionCode": "9"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.881375651656-dksihke73tspht1n2em6vr0idqgaqbo5"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "90cf25eb-7332-4489-83b4-65a9e1d9f9a2"
      }
    },
    "owner": "bertytechnologies"
  }
}
